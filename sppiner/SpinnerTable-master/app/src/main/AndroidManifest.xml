<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spinnertable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SpinnerTable"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Others"></activity>
        <activity android:name=".Chinese" />
        <activity android:name=".Contenental" />
        <activity android:name=".Italian" />
        <activity android:name=".Indian" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>